<% admin_breadcrumb(link_to plural_resource_name(Spree::Product), solidus_admin.products_path) %>
<% admin_breadcrumb(link_to plural_resource_name(Spree::Property), solidus_admin.properties_path) %>
<% admin_breadcrumb(@property.name) %>


<%= render partial: 'spree/shared/error_messages', locals: { target: @property } %>

<%= form_for [@property] do |f| %>
  <fieldset class="no-border-top">
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'solidus_admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
